<h1><%=@dojo.branch%>'s <%=@student.first_name%> <%=@student.last_name%></h1>
<h4>Email: <%=@student.email%></h4>
<h2>Cohort</h2>
<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <%@cohort.each do |student|%>
      <tr>
        <td><%=student.first_name%></td>
        <td><%=student.last_name%></td>
        <td><%=student.email%></td>
      </tr>
    <%end%>
  </tbody>
</table>
<p><a href="/dojos">Home</a> | <a href="/dojos/<%=@dojo.id%>/students/<%=@student.id%>/edit">Edit</a></p>
