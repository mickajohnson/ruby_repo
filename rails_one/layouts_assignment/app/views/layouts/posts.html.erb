<!DOCTYPE html>
<html>
<head>
  <title>Posts</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
  <div class="row">
    <div class="col-xs-4">
      <%= yield :posts %>
    </div>
    <div class="col-xs-4">
      <%= yield :new_post %>
    </div>
    <div class="col-xs-4">
      <%= yield :back_to_users %>
    </div>
  </div>
</div>
</body>
</html>
